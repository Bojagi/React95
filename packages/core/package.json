{
  "name": "@react95/core",
  "version": "2.10.4",
  "description": "Windows 95 styleguide",
  "main": "components/index.js",
  "repository": "git@github.com:React95/React95.git",
  "contributors": [
    "Allysson dos Santos <aallysson0@gmail.com>",
    "Gabriel Daltoso <ggdaltoso@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@react95/icons": "^0.3.0",
    "@styled-system/prop-types": "^5.1.5",
    "@testing-library/react-hooks": "^3.2.1",
    "@xstyled/styled-components": "^1.17.1",
    "icojs": "^0.14.0",
    "prop-types": "^15.7.2",
    "react-draggable": "^4.3.1",
    "react-virtualized": "^9.21.2",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@react95/clippy": "^0.1.1",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "@storybook/theming": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "fs-extra": "^9.0.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -s ./components/shared/assets",
    "build-storybook": "build-storybook -c .storybook -s ./components/shared/assets -o docs",
    "deploy-storybook": "storybook-to-ghpages",
    "build": "cross-env NODE_ENV=production babel ./components --out-dir dist --copy-files --ignore \"src/**/*.test.*\" --no-copy-ignored",
    "postbuild": "rimraf ./dist/**/__snapshots__",
    "prebuild": "rimraf ./dist",
    "prepublish": "yarn build",
    "postpublish": "yarn deploy-storybook --ci",
    "prepublishOnly": "node ../../scripts/prepublish.js"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
